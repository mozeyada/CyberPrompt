# CyberCQBench Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Application Environment
APP_ENV=dev
API_KEYS=your_api_key_here

# Database Configuration
MONGO_URI=mongodb://mongo:27017
MONGO_DB=genai_bench

# LLM API Keys (Required for experiments)
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GROQ_API_KEY=your_groq_api_key_here

# Pricing Configuration (AUD per 1K tokens)
# OpenAI Models
PRICE_INPUT.gpt4=0.03
PRICE_OUTPUT.gpt4=0.06
PRICE_INPUT.gpt4o=0.0075
PRICE_OUTPUT.gpt4o=0.03
PRICE_INPUT.gpt4o-mini=0.00015
PRICE_OUTPUT.gpt4o-mini=0.0006

# Anthropic Models
PRICE_INPUT.claude35=0.025
PRICE_OUTPUT.claude35=0.075
PRICE_INPUT.claude3-opus=0.045
PRICE_OUTPUT.claude3-opus=0.225
PRICE_INPUT.claude3-haiku=0.00075
PRICE_OUTPUT.claude3-haiku=0.00375